  G4_pursued : [0..1] init 0;
  G4_achieved : [0..1] init 0;

  [pursue_G4] G4_pursued=0 & G4_achieved=0 -> (G4_pursued'=1);
  [pursue_T5] G4_pursued=1 & G4_achieved=0 & R2=true -> true;

  [achieved_G4] G4_pursued=1 & (T5_achieved=1) -> (G4_pursued'=0) & (G4_achieved'=1);
  
  [skip_G4] G4_pursued=1 & T5_pursued=0 -> (G4_pursued'=0);
endmodule

formula G4_achievable = T5_achievable;

