formula G10_achievable = T6_achievable;

module G11
  G11_pursued : [0..1] init 0;

  [pursue_G11] G11_pursued=0 & G11_achieved_maintain=false & sampleLoaded -> (G11_pursued'=1);
  [pursue_T7] G11_pursued=1 & G11_achieved_maintain=false & T7_achieved=0 & sampleLoaded -> true;
  [pursue_T8] G11_pursued=1 & G11_achieved_maintain=false & T7_achieved=1 -> true;

  [achieved_G11] G11_pursued=1 & (T7_achieved=1 & T8_achieved=1) -> (G11_pursued'=0);
  
  [skip_G11] G11_pursued=1 & T7_pursued=0 & T8_pursued=0 -> (G11_pursued'=0);
endmodule

formula G11_achieved_maintain = analysisEnded;
formula G11_achievable = T7_achievable * T8_achievable;

module G12
  G12_pursued : [0..1] init 0;
  G12_achieved : [0..1] init 0;

  [pursue_G12] G12_pursued=0 & G12_achieved=0 -> (G12_pursued'=1);
  [pursue_T9] G12_pursued=1 & G12_achieved=0 & R1>=2 -> true;

  [achieved_G12] G12_pursued=1 & (T9_achieved=1) -> (G12_pursued'=0) & (G12_achieved'=1);
  
  [skip_G12] G12_pursued=1 & T9_pursued=0 -> (G12_pursued'=0);
