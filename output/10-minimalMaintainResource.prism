dtmc




module G0
  G0_pursued : [0..1] init 0;
  G0_achieved : [0..1] init 0;

  [pursue_G0] G0_pursued=0 & G0_achieved=0 -> (G0_pursued'=1);
  [pursue_G1] G0_pursued=1 & G0_achieved=0 & G1_achieved=0 -> true;
  [pursue_G2] G0_pursued=1 & G0_achieved=0 & G1_achieved=1 & G3_achieved=0 & G4_achieved=0 & G5_achieved=0 -> true;
  [pursue_G3] G0_pursued=1 & G0_achieved=0 & G1_achieved=1 & G2_achieved=1 & G4_achieved=0 & G5_achieved=0 -> true;
  [pursue_G4] G0_pursued=1 & G0_achieved=0 & G1_achieved=1 & G2_achieved=1 & G3_achieved=1 & G5_achieved=0 -> true;
  [pursue_G5] G0_pursued=1 & G0_achieved=0 & G1_achieved=1 & G2_achieved=1 & G3_achieved=1 & G4_achieved=1 -> true;

  [achieved_G0] G0_pursued=1 & (G1_achieved=1 & G2_achieved=1 & G3_achieved=1 & G4_achieved=1 & G5_achieved=1) -> (G0_pursued'=0) & (G0_achieved'=1);
  
  [skip_G0] G0_pursued=1 & G1_pursued=0 & G2_pursued=0 & G3_pursued=0 & G4_pursued=0 & G5_pursued=0 -> (G0_pursued'=0);
endmodule

formula G0_achievable = G1_achievable * G2_achievable * G3_achievable * G4_achievable * G5_achievable;

module G1
  G1_pursued : [0..1] init 0;
  G1_achieved : [0..1] init 0;

  [pursue_G1] G1_pursued=0 & G1_achieved=0 -> (G1_pursued'=1);
  [pursue_T1] G1_pursued=1 & G1_achieved=0 -> true;
  [pursue_T2] G1_pursued=1 & G1_achieved=0 -> true;

  [achieved_G1] G1_pursued=1 & (T1_achieved=1 & T2_achieved=1) -> (G1_pursued'=0) & (G1_achieved'=1);
  
  [skip_G1] G1_pursued=1 & T1_pursued=0 & T2_pursued=0 -> (G1_pursued'=0);
endmodule

formula G1_achievable = T1_achievable * T2_achievable;

module G2
  G2_pursued : [0..1] init 0;
  G2_achieved : [0..1] init 0;

  [pursue_G2] G2_pursued=0 & G2_achieved=0 & missionReady -> (G2_pursued'=1);
  [pursue_G6] G2_pursued=1 & G2_achieved=0 -> true;
  [pursue_G7] G2_pursued=1 & G2_achieved=0 & G7_achieved_maintain=false -> true;

  [achieved_G2] G2_pursued=1 & (G6_achieved=1 & G7_achieved_maintain=true) -> (G2_pursued'=0) & (G2_achieved'=1);
  
  [skip_G2] G2_pursued=1 & G6_pursued=0 & G7_pursued=0 -> (G2_pursued'=0);
endmodule

formula G2_achievable = G6_achievable * G7_achievable;

module G3
  G3_pursued : [0..1] init 0;
  G3_achieved : [0..1] init 0;
  G3_chosen : [0..3] init 0;

  [pursue_G3] G3_pursued=0 & G3_achieved=0 -> (G3_pursued'=1);
  [pursue_G9] G3_pursued=1 & G3_achieved=0 & G3_chosen!=2 & G3_chosen!=3 -> (G3_chosen'=1);
  [pursue_G10] G3_pursued=1 & G3_achieved=0 & G3_chosen!=1 & G3_chosen!=3 -> (G3_chosen'=2);
  [pursue_G11] G3_pursued=1 & G3_achieved=0 & G3_chosen!=1 & G3_chosen!=2 -> (G3_chosen'=3);

  [achieved_G3] G3_pursued=1 & (G9_achieved=1 | G10_achieved=1 | G11_achieved=1) -> (G3_pursued'=0) & (G3_achieved'=1);
  
  [skip_G3] G3_pursued=1 & G9_pursued=0 & G10_pursued=0 & G11_pursued=0 -> (G3_pursued'=0);
endmodule

formula G3_achievable = G9_achievable + G10_achievable + G11_achievable - (G9_achievable * G10_achievable * G11_achievable);

module G4
  G4_pursued : [0..1] init 0;
  G4_achieved : [0..1] init 0;
  G12_failed : [0..5] init 0;

  [pursue_G4] G4_pursued=0 & G4_achieved=0 -> (G4_pursued'=1);
  [pursue_G12] G4_pursued=1 & G4_achieved=0 -> (G12_failed'=min(5, G12_failed+1));
  [pursue_G13] G4_pursued=1 & G4_achieved=0 & G12_failed >= 2 -> true;

  [achieved_G4] G4_pursued=1 & (G12_achieved=1 | G13_achieved=1) -> (G4_pursued'=0) & (G4_achieved'=1);
  
  [skip_G4] G4_pursued=1 & G12_pursued=0 & G13_pursued=0 -> (G4_pursued'=0);
endmodule

formula G4_achievable = G12_achievable + G13_achievable - (G12_achievable * G13_achievable);

module G5
  G5_pursued : [0..1] init 0;
  G5_achieved : [0..1] init 0;

  [pursue_G5] G5_pursued=0 & G5_achieved=0 -> (G5_pursued'=1);
  [pursue_G14] G5_pursued=1 & G5_achieved=0 & G15_pursued=0 -> true;
  [pursue_G15] G5_pursued=1 & G5_achieved=0 & G14_pursued=0 -> true;

  [achieved_G5] G5_pursued=1 & (G14_achieved=1 | G15_achieved=1) -> (G5_pursued'=0) & (G5_achieved'=1);
  
  [skip_G5] G5_pursued=1 & G14_pursued=0 & G15_pursued=0 -> (G5_pursued'=0);
endmodule

formula G5_achievable = G14_achievable + G15_achievable - (G14_achievable * G15_achievable);

