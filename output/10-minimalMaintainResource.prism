  [pursue_G7] G7_pursued=0 & G7_achieved_maintain=false & inFlight -> (G7_pursued'=1);
  [pursue_T4] G7_pursued=1 & G7_achieved_maintain=false -> true;

  [achieved_G7] G7_pursued=1 & (T4_achieved=1) -> (G7_pursued'=0);
  
